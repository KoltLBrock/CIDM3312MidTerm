@model LoginViewModel
@{
    Layout = "_Layout";
    ViewData["Title"] = "Account Created";
}
<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=divice-width">
        <title>Account Created</title>
    </head>
    <body>
        <h1>@ViewData["Title"]</h1>
        <p>Account created for @Model.userName</p>
        <a asp-controller="User" asp-action="Login" class="btn btn-primary">Now Log In!</a>
    </body>
</html>